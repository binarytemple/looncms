<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<!-- Default Cache ... -->
	<defaultCache maxElementsInMemory="50000" eternal="false" overflowToDisk="false" timeToIdleSeconds="300"
		timeToLiveSeconds="300" diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />
	<!-- Our Cache .. -->
	<cache name="SimplePageCachingFilter" maxElementsInMemory="0" eternal="false" timeToIdleSeconds="600"
		timeToLiveSeconds="600" overflowToDisk="true" />
	<cache name="adminServiceCache" maxElementsInMemory="100" eternal="true" timeToIdleSeconds="600"
		timeToLiveSeconds="14400" overflowToDisk="true" />
</ehcache>